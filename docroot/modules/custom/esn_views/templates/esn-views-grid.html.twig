{#
/**
 * Default theme implementation for Views UR Grid.
 *
 * @ingroup themeable
 */
#}
{% embed '@organisms/grid/grid.twig'
  with {
    grid_type: 'card',
    item_type: 'card',
    items: rows,
    page_layout_modifier: columns
  }
%}
  {% block grid_content %}
    {% for row in rows %}
      {{ row.content }}
    {% endfor %}
  {% endblock %}
{% endembed %}
