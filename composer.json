    "config": {
        "process-timeout": 0,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "neronmoon/scriptsdev": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "conflict": {
        "drupal/drupal": "*",
        "twig/twig": "3.14.1"
    },
    "extra": {
        "composer-exit-on-patch-failure": false,
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": {
                    "append": "docroot/robots_dist.txt"
                }
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "dupal/media_entity": {
                "rename": "https://www.drupal.org/files/issues/2020-02-14/rename-media-bundles-before-file-update-3113671-2.patch"
            },
            "drupal/node_edit_protection": {
                "Doesn't work with paragraphs": "https://www.drupal.org/files/issues/2022-06-07/3284405_0.patch"
            },
            "drupal/claro": {
                "dropbottum": "https://www.drupal.org/files/issues/2022-06-21/claro-dropbottum-3217868-20.patch",
                "function deprected": "https://www.drupal.org/files/issues/2020-02-07/attributes-key-missing-hash-3111390-3.patch",
                "claro templates": "https://www.drupal.org/files/issues/2022-09-28/3312270-claro-menu-twig-fix.patch"
            },
            "drupal/webform": {
                "2971460-1 getUrl() webform correction ": "https://www.drupal.org/files/issues/2021-10-14/2971460-1.patch",
                "polyfill.io Library is no longer considered safe to use": "https://www.drupal.org/files/issues/2024-03-15/webform-remove-polyfill-mr-419.diff.patch"
            },
            "drupal/core": {
                "path-matcher": "https://www.drupal.org/files/issues/2022-07-18/ASGS-4866-php8-compatibility_3.patch"
            },
            "drupal/layout_builder_st": {
                "ComponentPluginTranslate::isTranslation() must implement interface": "https://www.drupal.org/files/issues/2019-07-24/isTranslation-null-given.patch"
            },
            "drupal/tb_megamenu": {
                "Fix submenu rendering": "patches/tb_megamenu.patch",
                "Fix menu hover on touch devices": "patches/tb_megamenu_touch.patch"
            },
            "drupal/seckit": {
                "Change Feature Policy to Permissions Policy (D8/D9)": "https://www.drupal.org/files/issues/2023-08-11/seckit-3185024-permissions-policy-22_0.patch"
            },
            "drupal/ckeditor_videodetector": {
                "Automated Drupal Rector fixes": "https://www.drupal.org/files/issues/2020-05-23/ckeditor_videodetector.1.x-dev.rector.patch"
            },
            "drupal/lightning_core": {
                "Fix getDescription declaration": "patches/lightning_core_getDescription.patch",
                "ConfigEntityDescriptionTrait is not compatible with EntityDisplayModeBase": "patches/lighting_core_EntityDescriptionTrait.patch"
            },
            "drupal/lightning_workflow": {
                "Fix circular reference": "patches/lightning_workflow_circular_reference.patch",
                "Taxonomy_term__scheduled_transition_date doesn't exist": "https://www.drupal.org/files/issues/2024-07-05/add-base-fields-to-new-content-types-supported-by-content-moderation-3459341-2.patch"
            },
            "drupal/externalauth": {
                "Add missing table after upgrade": "patches/externalauth_add_missing_table.patch"
            },
            "drupal/lightning_media": {
                "The mapping definition is not accurate": "patches/mapping-definition-entity-browser-upload-validators-3456919.patch"
            }
        },
        "patches-ignore": {
            "drupal/lightning_core": {
                "drupal/core": {
                    "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2869592-remove-update-warning-7.patch"
                },
                "drupal/calendar": {
                    "#3396700: Creation of dynamic properties is deprecated": "https://www.drupal.org/files/issues/2023-10-26/calendar-dynamic-properti..."
                }
            }
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "cp docroot/.htaccess_dist docroot/.htaccess",
            "echo '' > docroot/modules/contrib/jquery_ui/assets/vendor/jquery.ui/themes/base/theme.css",
            "echo '' > docroot/core/modules/image/image.post_update.php"
        ],
        "post-update-cmd": [
            "cp docroot/.htaccess_dist docroot/.htaccess",
            "echo '' > docroot/modules/contrib/jquery_ui/assets/vendor/jquery.ui/themes/base/theme.css",
            "echo '' > docroot/core/modules/image/image.post_update.php"
        ],
        "post-create-project-cmd": "rm -r -f .travis.yml .travis-ci",
        "quick-start": [
            "composer install",
            "php docroot/core/scripts/drupal quick-start lightning --no-interaction"
        ],
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 < ../patches/htaccess-ips-blocked.patch",
            "rm docroot/.htaccess.orig"
        ]
    }
}
