    "require-dev": {
        "enlightn/security-checker": "^1.10",
        "phpspec/prophecy-phpunit": "^2"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "neronmoon/scriptsdev": true
        }
    },
    "extra": {
        "scripts-dev": {
            "post-install-cmd": [
                "./vendor/bin/grumphp git:init"
            ],
            "post-update-cmd": [
                "./vendor/bin/grumphp git:init"
            ]
        },
        "composer-exit-on-patch-failure": false,
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": {
                    "append": "docroot/robots_dist.txt"
                }
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "dupal/media_entity": {
                "rename": "https://www.drupal.org/files/issues/2020-02-14/rename-media-bundles-before-file-update-3113671-2.patch"
            },
            "drupal/node_edit_protection": {
                "Doesn't work with paragraphs": "https://www.drupal.org/files/issues/2022-06-07/3284405_0.patch"
            },
            "drupal/claro": {
                "dropbottum": "https://www.drupal.org/files/issues/2022-06-21/claro-dropbottum-3217868-20.patch",
                "function deprected": "https://www.drupal.org/files/issues/2020-02-07/attributes-key-missing-hash-3111390-3.patch",
                "claro templates": "https://www.drupal.org/files/issues/2022-09-28/3312270-claro-menu-twig-fix.patch"
            },
            "drupal/textfield_counter": {
                "incompatible claro": "https://www.drupal.org/files/issues/2022-08-04/textfield-counter-incompatible-with-claro-3182675-10.patch",
                "function deprected": "https://www.drupal.org/files/issues/2022-04-27/deprecated-static-function-call-in-trait-3272128.patch"
            },
            "drupal/webform": {
                "2971460-1 getUrl() webform correction ": "https://www.drupal.org/files/issues/2021-10-14/2971460-1.patch",
                "polyfill.io Library is no longer considered safe to use": "https://www.drupal.org/files/issues/2024-03-15/webform-remove-polyfill-mr-419.diff.patch"
            },
            "drupal/core": {
                "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch",
                "path-matcher": "https://www.drupal.org/files/issues/2022-07-18/ASGS-4866-php8-compatibility_3.patch",
                "operations": "https://www.drupal.org/files/issues/2021-11-23/2473873-97.patch",
                "claro templates": "https://www.drupal.org/files/issues/2022-09-28/3312270-claro-menu-twig-fix.patch",
                "Undefined index: region on layout": "https://www.drupal.org/files/issues/2019-12-16/undefined_index_region_field_layout-3053906-30.patch",
                "MethodNotAllowedException thrown on POST requests if user has set admin language in their profile": "https://www.drupal.org/files/issues/2019-04-15/route-method-exception-path-fix-3048288.patch"
            },
            "drupal/layout_builder_st": {
                "ComponentPluginTranslate::isTranslation() must implement interface": "https://www.drupal.org/files/issues/2019-07-24/isTranslation-null-given.patch"
            },
            "drupal/tb_megamenu": {
                "Fix submenu rendering": "patches/tb_megamenu.patch"
            },
            "drupal/seckit": {
                "Change Feature Policy to Permissions Policy (D8/D9)": "https://www.drupal.org/files/issues/2023-08-11/seckit-3185024-permissions-policy-22_0.patch"
            },
            "drupal/ckeditor_videodetector": {
                "Automated Drupal Rector fixes": "https://www.drupal.org/files/issues/2020-05-23/ckeditor_videodetector.1.x-dev.rector.patch"
            },
            "drupal/geolocation": {
                "deg2rad fixes": "https://www.drupal.org/files/issues/2022-01-11/3257751-5.patch"
            },
            "drupal/oauth2_server": {
                "OAuth2Storage::setAuthorizationCode": "https://www.drupal.org/files/issues/2023-06-27/11.patch",
                "php-8.1 compatibility": "https://www.drupal.org/files/issues/2023-06-07/3307940-php-8.1-compatibility-16.patch"
            },
            "drupal/gin_toolbar": {
                "node local tasks iin toolbar": "https://www.drupal.org/files/issues/2022-02-11/3263805-41.patch"
            },
            "drupal/block_class": {
                "TypeError: array_keys(): Argument #1 ($array) must be of type array": "https://www.drupal.org/files/issues/2023-07-11/third_party_settings_3343165.patch"
            },
            "drupal/layout_builder_browser": {
                "Undefined array key in": "https://www.drupal.org/files/issues/2023-03-29/3351162.patch"
            },
            "drupal/advagg": {
                "Fix PHP 8.1 deprecated code": "patches/advagg_PHP81_deprecated.patch"
            },
            "drupal/simple_oauth": {
                "Undefined constant self::MAIN_REQUEST in BasicAuthSwap::handle() declaration": "patches/simple_oauth-3391076.patch"
            }
        },
        "patches-ignore": {
            "drupal/lightning_core": {
                "drupal/core": {
                    "1356276 - Allow profiles to define a base/parent profile and load them in the correct order": "https://www.drupal.org/files/issues/2019-11-05/1356276-531-8.8.x-4.patch",
                    "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2869592-remove-update-warning-7.patch",
                    "2914389 - Allow profiles to exclude dependencies of their parent": "https://www.drupal.org/files/issues/2018-07-09/2914389-8-do-not-test.patch"
                }
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "cp docroot/.htaccess_dist docroot/.htaccess",
            "echo '' > docroot/modules/contrib/jquery_ui/assets/vendor/jquery.ui/themes/base/theme.css",
            "echo '' > docroot/core/modules/image/image.post_update.php"
        ],
        "post-update-cmd": [
            "cp docroot/.htaccess_dist docroot/.htaccess",
            "echo '' > docroot/modules/contrib/jquery_ui/assets/vendor/jquery.ui/themes/base/theme.css",
            "echo '' > docroot/core/modules/image/image.post_update.php"
        ],
        "post-create-project-cmd": "rm -r -f .travis.yml .travis-ci",
        "quick-start": [
            "composer install",
            "php docroot/core/scripts/drupal quick-start lightning --no-interaction"
        ],
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 < ../patches/htaccess-ips-blocked.patch",
            "rm docroot/.htaccess.orig"
        ]
    }
}
